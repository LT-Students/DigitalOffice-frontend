{
  "name": "digital-office-frontend",
  "version": "5.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "services": "ng serve && start.bat",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "branch": "sh .githooks/pre-commit",
    "prettier": "prettier --write src/**/*.{ts,scss}",
    "generate.auth": "ng-openapi-gen -c swagger/config/auth.json",
    "generate.company": "ng-openapi-gen -c swagger/config/company.json",
    "generate.file": "ng-openapi-gen -c swagger/config/file.json",
    "generate.image": "ng-openapi-gen -c swagger/config/image.json",
    "generate.message": "ng-openapi-gen -c swagger/config/message.json",
    "generate.news": "ng-openapi-gen -c swagger/config/news.json",
    "generate.project": "ng-openapi-gen -c swagger/config/project.json",
    "generate.rights": "ng-openapi-gen -c swagger/config/rights.json",
    "generate.time": "ng-openapi-gen -c swagger/config/time.json",
    "generate.user": "ng-openapi-gen -c swagger/config/user.json",
    "generate-api": "node swagger/generate.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.4",
    "@angular/cdk": "~12.2.4",
    "@angular/common": "~12.2.4",
    "@angular/compiler": "~12.2.4",
    "@angular/core": "~12.2.4",
    "@angular/forms": "~12.2.4",
    "@angular/localize": "^12.2.4",
    "@angular/material": "^12.2.4",
    "@angular/material-moment-adapter": "^12.2.4",
    "@angular/platform-browser": "~12.2.4",
    "@angular/platform-browser-dynamic": "~12.2.4",
    "@angular/router": "~12.2.4",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "bootstrap": "^4.5.2",
    "canvasjs": "^1.8.3",
    "chart.js": "^3.5.1",
    "moment": "^2.27.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.4",
    "@angular-eslint/builder": "0.6.0-beta.0",
    "@angular-eslint/eslint-plugin": "0.6.0-beta.0",
    "@angular-eslint/eslint-plugin-template": "0.6.0-beta.0",
    "@angular-eslint/schematics": "^0.6.0-beta.0",
    "@angular-eslint/template-parser": "^0.6.0-beta.0",
    "@angular/cli": "~12.2.4",
    "@angular/compiler-cli": "~12.2.4",
    "@angular/language-service": "~12.2.4",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^10.5.0",
    "ng-openapi-gen": "^0.16.0",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh .githooks/pre-commit && npx lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.{json}": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint --fix"
    ],
    "src/**/*.{ts,html}, !src/data/api/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
