    "@angular/router": "~10.2.1",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "bootstrap": "^4.5.2",
    "canvasjs": "^1.8.3",
    "chart.js": "^2.9.3",
    "moment": "^2.27.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.11.2"
