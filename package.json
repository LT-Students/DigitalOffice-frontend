    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
