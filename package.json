{
  "name": "digital-office-frontend",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "branch": "sh .githooks/pre-commit",
    "prettier": "prettier --write src/**/*.{ts,scss}",
    "generate.admin": "ng-openapi-gen -c swagger/config/admin.json",
    "generate.education": "ng-openapi-gen -c swagger/config/education.json",
    "generate.auth": "ng-openapi-gen -c swagger/config/auth.json",
    "generate.company": "ng-openapi-gen -c swagger/config/company.json",
    "generate.department": "ng-openapi-gen -c swagger/config/department.json",
    "generate.feedback": "ng-openapi-gen -c swagger/config/feedback.json",
    "generate.file": "ng-openapi-gen -c swagger/config/file.json",
    "generate.filter": "ng-openapi-gen -c swagger/config/filter.json",
    "generate.gateway": "ng-openapi-gen -c swagger/config/gateway.json",
    "generate.image": "ng-openapi-gen -c swagger/config/image.json",
    "generate.message": "ng-openapi-gen -c swagger/config/message.json",
    "generate.news": "ng-openapi-gen -c swagger/config/news.json",
    "generate.office": "ng-openapi-gen -c swagger/config/office.json",
    "generate.position": "ng-openapi-gen -c swagger/config/position.json",
    "generate.project": "ng-openapi-gen -c swagger/config/project.json",
    "generate.rights": "ng-openapi-gen -c swagger/config/rights.json",
    "generate.time": "ng-openapi-gen -c swagger/config/time.json",
    "generate.user": "ng-openapi-gen -c swagger/config/user.json",
    "generate-api": "node swagger/generate.js",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.5",
    "@angular/cdk": "^14.2.4",
    "@angular/common": "^14.2.5",
    "@angular/compiler": "^14.2.5",
    "@angular/core": "^14.2.5",
    "@angular/forms": "^14.2.5",
    "@angular/localize": "^14.2.5",
    "@angular/material": "^14.2.4",
    "@angular/material-luxon-adapter": "^14.2.4",
    "@angular/material-moment-adapter": "^14.2.4",
    "@angular/platform-browser": "^14.2.5",
    "@angular/platform-browser-dynamic": "^14.2.5",
    "@angular/router": "^14.2.5",
    "@editorjs/delimiter": "^1.2.0",
    "@editorjs/editorjs": "^2.23.2",
    "@editorjs/header": "^2.6.1",
    "@editorjs/image": "^2.6.1",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/nested-list": "^1.0.2",
    "@editorjs/quote": "^2.4.0",
    "@editorjs/underline": "^1.0.0",
    "@ngx-loading-bar/core": "^6.0.2",
    "@ngx-loading-bar/router": "^6.0.2",
    "@types/luxon": "^2.3.0",
    "bootstrap": "^4.5.2",
    "browser-image-compression": "^2.0.0",
    "canvasjs": "^1.8.3",
    "chart.js": "^3.7.1",
    "editorjs-text-alignment-blocktune": "^1.0.3",
    "libphonenumber-js": "^1.9.49",
    "luxon": "^2.3.1",
    "moment": "^2.29.4",
    "ngx-image-cropper": "^6.2.2",
    "ngx-infinite-scroll": "^14.0.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.5",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "^14.1.2",
    "@angular/cli": "^14.2.5",
    "@angular/compiler-cli": "^14.2.5",
    "@angular/language-service": "^14.2.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.18.12",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.2",
    "editorjs-undo": "^0.3.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.0",
    "jasmine-core": "~4.4.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "lint-staged": "^10.5.0",
    "ng-openapi-gen": "^0.20.0",
    "prettier": "^2.6.0",
    "protractor": "~7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh .githooks/pre-commit && sh .husky/pre-commit"
    }
  },
  "lint-staged": {
    "*.{json}": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{ts,html}": [
      "prettier --write",
      "./node_modules/.bin/eslint --fix"
    ]
  }
}
